{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@zeppelin/*": [
        "./src/app/*",
        "./src/environments/*"
      ],
      "@zeppelin/helium": [
        "out-lib/zeppelin-helium"
      ],
      "@zeppelin/helium/*": [
        "out-lib/zeppelin-helium/*"
      ],
      "@zeppelin/visualization": [
        "out-lib/zeppelin-visualization"
      ],
      "@zeppelin/visualization/*": [
        "out-lib/zeppelin-visualization/*"
      ],
      "@zeppelin/sdk": [
        "out-lib/zeppelin-sdk"
      ],
      "@zeppelin/sdk/*": [
        "out-lib/zeppelin-sdk/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}